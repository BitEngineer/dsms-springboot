<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lonton.dsms.security.mapper.SecurityMapper" >
	<select parameterType="java.util.Map" resultType="int">
		select count(1) from A_STAFF where STAFF_CODE = #{staffCode} and PASSWORD = #{password}
	</select>
</mapper>